<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zhulang.waveedu.chat.dao.BasicUserMapper">


    <select id="getBasicUserInfo" resultType="com.zhulang.waveedu.chat.pojo.BasicUserInfo">
        select *
        from wave_edu.basic_user_info
        where id = #{userId}
    </select>
</mapper>