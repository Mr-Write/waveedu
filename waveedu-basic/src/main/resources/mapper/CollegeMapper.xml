<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zhulang.waveedu.basic.dao.CollegeMapper">

    <select id="selectNamesLikeName" resultType="java.lang.String">
        SELECT `name`
        FROM `basic_college`
        WHERE `name` LIKE #{name}"%"
    </select>
</mapper>